<body>
    <nav>
        <!-- Login Modal -->
    <div class="modal" id="loginModal">
        <div class="modal-content">
            <span class="close" id="closeLogin">&times;</span>
            <img class="img1" src="https://i.pinimg.com/originals/d3/d1/75/d3d175e560ae133f1ed5cd4ec173751a.png"
                alt="pin logo" />
            <p class="header">Welcome to pinterest</p>

            <%if(error.length> 0){%>
                <p class="error">
                    <%= error %>
                </p>
                <%} %>
                    <form action="/login" method="post">
                        <input type="text" name="username" placeholder="User Name" class="detail" /><br />
                        <input type="password" name="password" placeholder="Password" class="detail" />
                        <input type="submit" class="btn int" value="Login">
                    </form>
                    <a href="/forgot">Forgot your password?</a>
                    <p class="or">OR</p>
                    <button class="btn fbk">
                        <i class="fab fa-facebook fa-lg" style="color: white; padding-right: 10px"></i><a
                            href="#">Continue with
                            Facebook</a></button><br />
                    <button class="btn ggl">
                        <i class="fab fa-google" style="color: rgb(255, 255, 255); padding-right: 10px"></i><a
                            href="#">Continue with
                            Google</a>
                    </button>
                    <footer>
                        <p>
                            By continuing, you agree to Pinterest's
                            <b>Terms of Service, Privacy policy.</b>
                            <hr />
                            <span>Not on Pinterest yet?</span><a href="#" id="signupLink">Sign up</a>
                        </p>
                    </footer>
        </div>
    </div>
    <!-- Register Modal -->
    <div class="modal" id="registerModal">
        <div class="modal-content">
            <span class="close" id="closeRegister">&times;</span>
            <img src="https://i.pinimg.com/originals/d3/d1/75/d3d175e560ae133f1ed5cd4ec173751a.png" alt="pin logo"
                class="img1" />
            <p class="header">Welcome to pinterest</p>

            <%if(errortwo.length> 0){%>
                <p class="error">
                    <%= errortwo %>
                </p>
                <%} %>

            <form action="/register" method="post">
                <input type="email" name="email" placeholder="Email" class="detail" /><br />
                <input type="text" name="username" placeholder="User Name" class="detail" /><br />
                <input type="text" name="Fullname" placeholder="Full Name" class="detail" /><br />
                <input type="password" name="password" placeholder="Password" class="detail" />
                <input type="submit" class="btn int" value="Register Account">
            </form>

            <p class="or">OR</p>
            <button class="btn fbk">
                <i class="fab fa-facebook fa-lg" style="color: white; padding-right: 10px"></i><a href="#">Continue with
                    Facebook</a></button><br />
            <button class="btn ggl">
                <i class="fab fa-google" style="color: rgb(255, 255, 255); padding-right: 10px"></i><a href="#">Continue
                    with
                    Google</a>
            </button>

            <footer>
                <p>
                    By continuing, you agree to Pinterest's
                    <b>Terms of Service, Privacy policy.</b>
                    <hr />
                    <span>Already on Pinterest?</span><a href="#" id="loginLink">Login</a>
                </p>
            </footer>
        </div>
    </div>
